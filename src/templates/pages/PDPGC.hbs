        <form class="row" id="gift-card-form">
          <div class="col-6 form-group">
            <label class="bold">Amount<span class="brand-color"> *</span></label>
            <input type="text" name="amount" class="form-control contact black-border " id="exampleFormControlInput1" placeholder="Other amount">
          </div>
          <div class="col-6 form-group">
             <label class="bold">Amount in US<span class="brand-color"> *</span></label>
            <input type="text" name="amount" class="form-control contact black-border" id="exampleFormControlInput1" placeholder="Other amount">
          </div>
          <div class="col-6 form-group">
             <label class="bold">Sender Name<span class="brand-color"> *</span></label>
            <input type="text" name="name" class="form-control contact name black-border" id="exampleFormControlInput1">
          </div>
          <div class="col-6 form-group">
             <label class="bold">Recipient Name<span class="brand-color"> *</span></label>
            <input type="text" name="name" class="form-control contact name black-border" id="exampleFormControlInput1">
          </div>
          <div class="col-12 form-group">
            <label class="bold">Message:</label>
            <textarea class="form-control contact required comment black-border" id="exampleFormControlTextarea1" rows="3"></textarea>
        </div>
        
        
         <input type="number" name="qty" class="qty-input ml-3" placeholder="qty.">
        <button type="submit" class="btn add-to-cart-btn inline">Add to Cart</button>
       </form>
      
        <button type="submit" class="btn checkout-btn mt-0">Checkout with PayPal</button>
        <hr>
        <div class="inline">
